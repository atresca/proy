header {
	height:101px;
	width:100%;
	padding:0px;
	background-image: -moz-linear-gradient(bottom, #83827c 0%, #65646c 25%); /* Mozilla*/
	background-image: -ms-linear-gradient(bottom, #83827c 0%, #65646c 25%); /* explorer 10*/
	background-image: -webkit-linear-gradient(bottom, #83827c 0%, #65646c 25%); /* Chrome - Safari*/
	box-shadow:inset 0 -1px 0px #646664;
}

header .wrapper{
	width:1150px;
	margin:auto;
}

header nav {
	
}

header img {
	display:block;
	float:left;
	margin: 7px 0 0 120px;	
}

header nav ul {
	float:left;
	position:relative;
	text-align:center;
	margin:45px 0px 0px 35px;
}

header nav ul li {
	display:inline;
	padding: 7px 13px 10px 13px;
	height: 21px;
	border-right: solid 1px #5c5b54;
	border-left: solid 1px #847e7c;
}


/*Hover effect*/
header nav ul li:hover a {
	color:#c3d62c;
	border-bottom: #c3d62c 1px dotted;
}

/*To make correct separators of menu options*/
header nav ul li:first-child {
	border-left:none;
}

header nav ul li:last-child {
	border-right:none;
}
/**/	

	
/* A different class for active menu option*/
header nav ul li a.active {
	border-bottom: solid 4px #c3d62c;
	font-weight:bold;
}

header nav ul li a.active:hover {
	border-bottom: solid 4px #c3d62c;
	font-weight:bold;
}

header nav ul li a {
	color:white;
	font-family: verdana, arial, helvetica, sans-serif; 
	font-size: 13px;
	text-decoration:none;
	padding:0 3px 4px 3px;
}

header span {
	color:#b7d02c;
	text-decoration:underline;
}

header p {
	float:right;
 	font-family: verdana, arial, helvetica, sans-serif; 
 	font-size:12px;
 	color: white;
 	margin:48px 100px 0px 0px;
}

