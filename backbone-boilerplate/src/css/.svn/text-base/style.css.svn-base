/*Form Container*/
form{	
	width: 700px;
	font-family: Arial;
}

/*Gives the style for the main section*/
.dashboard {
	border: 1px solid #BBBBBB;
	border-radius: 5px;
	box-shadow: 0px 0px 1px 0px gray;
	float:left;
}

/*Creates a dotted line under the title*/
.dashboard .title {
	margin-top:20px;
	margin-bottom:10px;
	border-bottom: 1px dotted #EAEAEA ;
}

/*Gives the style for the title*/
.dashboard .title h1 {
	margin: 10px 20px 10px 20px;
	font-size: 25px;
}

/*Gives the style for the inputs*/
.items article input {
	border: 0px;
	margin-left: 15px;
	width: 50%;
}

/*Gives the style for the inputs*/
.items .solo input {
	border: 0px;
	margin-left: 0px;
}

/*Give the style for the border of each input*/
.items article {
	border: 1px solid #BBBBBB;
	border-radius: 5px;
	margin: 10px 20px 10px 20px;
	padding: 10px;
}

/*Give the style for the border of each input*/
.items .solo {
	border: 1px solid #BBBBBB;
	border-radius: 5px;
	margin: 10px 20px 10px 20px;
	padding: 10px;
}

.items p {
	margin: 5px 20px 5px 10px;
	padding: 5px 10px 5px 10px;
}
/*Creates a dotted line under the last input*/
.dashboard .button {
	margin-top:10px;
	margin-bottom:10px;
	border-top: 1px dotted #EAEAEA;
}

/*This gives the style for the button Start Planning */
.dashboard .button .planning {
	margin: 10px 15px 0px 20px;
	padding: 6px 35px;
	border-color: #CAD7DE;
	border-radius: 4px;
	color: white;
	font-size: 16px;
	background: -webkit-linear-gradient(top, #99cae3 40%, #5bb3e1 80%); /*Chrome*/
	background: -moz-linear-gradient(top, #81BEDD 40%, #3399CC 80%); /*Firefox*/
}

/*This gives the style for the button Start Planning when mouse is over*/
 .dashboard .button .planning:hover, .tripName .button:hover, .breadcrumb .btn:hover, .loginWrapper .loginBtn:hover  {
	background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #5bb3e1), color-stop(1, #99cae3) ); /*Chrome*/
	background: -moz-linear-gradient( center top, #3399CC 5%, #81BEDD 100% ); /*Firefox*/
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#3399CC', endColorstr='#81BEDD');
	background-color: #378de5;
}

/*This gives the style for the button Start Planning when is active*/
.dashboard .button .planning:active, .tripName .button:active{
	position: relative;
	top: 1px;
}

/*This gives the style for the button Add more details */
.dashboard .button .details {
	margin: 10px 15px 0px 0px;
	padding: 6px 35px;
	border-color: #DFDFDF;
	border-radius: 4px;
	color: black;
	font-size: 16px;
	background: -webkit-linear-gradient(top, #FFFFFF 40%, #dfdfdf 80%);
	background: -moz-linear-gradient(top, #FFFFFF 40%, #dfdfdf 80%);
}

/*This gives the style for the button Add more details when mouse is over*/
.dashboard .button .details:hover {
	background: -webkit-gradient( linear, left top, left bottom, color-stop(0.05, #dfdfdf), color-stop(1, #FFFFFF) );
	background: -moz-linear-gradient( center top, #dfdfdf 5%, #FFFFFF 100% );
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#dfdfdf', endColorstr='#FFFFFF');
	background-color: #dfdfdf;
}

/*This gives the style for the button Add more details when is active*/
.dashboard .button .details:active {
	position: relative;
	top: 1px;
}

/*Gives the style for the "Other steps are optional"*/
.dashboard .button span{
	margin-top: 20px;
	margin-right:20px;
	color: #999999;
	float: right;
	font-size: 11px;
	font-weight: 600;
}

/*Gives the style for the "Click here"*/
.dashboard .button span a{
	font-size: 11px;
	margin-left: 5px;	
	font-weight: 100;
	 color: #2d86b2;
}

/*Makes the content of the input-placeholder italic*/
::-moz-placeholder {  /* Firefox 19+ */
  font-style: italic;
}

:-ms-input-placeholder {  /*IE*/
 font-style: italic;
}

::-webkit-input-placeholder { /*Chrome*/
  font-style: italic;
}

:-moz-placeholder { /* Firefox 18- */
  font-style: italic;
}
