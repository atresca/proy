.wrapperContainer {
	width: 990px;
	margin: 30px auto;
}

/* --------------------LOGIN START-------------------------*/
.login {
	background: -webkit-linear-gradient(top, #908f8f 75%, #C4C4C4);
	background: -moz-linear-gradient(top, #908f8f 75%, #C4C4C4);
	box-shadow: inset 0 -1px 0px #646664;
	padding: 10px 0px;
	width: 100%;
	text-align: center;
}
/* Wrapper of the header items */
.headerWrapper {
	margin: auto;
}
/* Start login section */
.loginWrapper {
	border-radius: 6px;
	border: 2px solid #CCCCCC;
	width: 800px;
	overflow: auto;
	margin: 60px auto 0px auto;
}

/* Any login element */
.loginWrapper * {
	display: block;
}

/* Textfields inside the login section.*/
.loginWrapper .inputText {
	border-radius: 5px;
	border-color: silver;
	margin: 35px auto 15px auto;
	padding: 10px 5px 10px 5px;
	width: 92%;
	font-size: 17px;
}

/* login button */
.loginWrapper .loginBtn {
	margin: 21px 15px 18px 0px;
	padding: 6px 35px;
	border-color: #CAD7DE;
	border-radius: 4px;
	float: right;
	color: white;
	font-size: 16px;
	background: -webkit-linear-gradient(top, #99cae3 40%, #5bb3e1 80%);
	background: -moz-linear-gradient(top, #99cae3 40%, #5bb3e1 80%);
}

/* styling placeholders */
.loginWrapper ::-webkit-input-placeholder {/* Chrome and Safari */
	font-style: italic;
	color: #CBCBCB;
}

.loginWrapper :-moz-placeholder {/* Mozilla 18- */
	font-style: italic;
	color: #CBCBCB;
}

.loginWrapper ::-moz-placeholder {/* Mozilla 19+ */
	font-style: italic;
	color: #CBCBCB;
}
/* end of styling placeholders */

/* end login section */
/* --------------------LOGIN END-------------------------*/
/* --------------------BREADCRUM START-------------------------*/
body {
	font-family: sans-serif;
}

.breadcrumb {
	width: 240px;
	border: 2px solid #43A2CE;
	border-radius: 6px;
	position: relative;
	background: white;
	border: 2px solid #43A2CE;
	margin-left: 25px;
	float:left;
}
/**
 * 	Style for the arrow.
 */
.breadcrumb:after, .breadcrumb:before {
	right: 100%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
}

.breadcrumb:after {
	border-color: rgba(213, 213, 213, 0);
	border-right-color: white;
	border-width: 15px;
	top: 25px;
	margin-top: -15px;
}
.breadcrumb:before {
	border-color: rgba(194, 225, 245, 0);
	border-right-color: #43A2CE;
	border-width: 18px;
	top: 25px;
	margin-top: -18px;
}

/**
 * 	End of style for arrow.
 */

/**
 * 	General style for the breadcrumb
 */
.breadcrumb ul {
	vertical-align: middle;
	list-style: none;
	padding: 0px;
	margin: 0;
}

.breadcrumb li {
	height: 50px;
	border-top: 1px dotted gray;
}

.breadcrumb span {
	color: #5C5D5C;
	font-size: 12px;
	font-weight: 700;
	position: relative;
	top: 15px;
	left: 60px;
}
.breadcrumb a {
	text-decoration: none;
}
.breadcrumb h4 {
	padding-left: 40px;
	margin: 15px auto;
	color: #43A2CE;
}

/**
 * 	Style for the enabled elements.
 */
.breadcrumb a.enabled > li.details {
	background: url(../images/icons/question.png) no-repeat 25px 15px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

.breadcrumb a.enabled > li.car {
	background: url(../images/icons/carb.png) no-repeat 20px 10px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

.breadcrumb a.enabled > li.flight {
	background: url(../images/icons/airplaneb.png) no-repeat 20px 10px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

.breadcrumb a.enabled > li.hotel {
	background: url(../images/icons/homeb.png) no-repeat 20px 10px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

.breadcrumb a.enabled > li.perdiem {
	background: url(../images/icons/coins.png) no-repeat 20px 10px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

.breadcrumb a.enabled > li.transfer {
	background: url(../images/icons/bus.png) no-repeat 20px 10px, url(../images/icons/chevron-right.png) no-repeat scroll right 20px center transparent;
}

/**
 * 	Hover style for enabled elements.
 */
.breadcrumb a.enabled:hover span {
	color: #43A2CE;
}

/**
 * 	Style for the disabled elements.
 */
.breadcrumb a.disabled > li.details {
	cursor: default;
	background: rgb(233,238,244); /* Old browsers */
	background: url(../images/icons/questionnb.png) no-repeat 25px 15px, -moz-linear-gradient(top, rgba(233,238,244,1) 0%, rgba(252,254,252,1) 100%);/* FF3.6+ */
	background: url(../images/icons/questionnb.png) no-repeat 25px 15px, -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(233,238,244,1)), color-stop(100%,rgba(252,254,252,1)));/* Chrome,Safari4+ */
	background: url(../images/icons/questionnb.png) no-repeat 25px 15px, -webkit-linear-gradient(top, rgba(233,238,244,1) 0%,rgba(252,254,252,1) 100%);/* Chrome10+,Safari5.1+ */
